# Private key for on-chain writes (hex-encoded ECDSA key, no 0x prefix)
ETH_PRIVATE_KEY_HEX=replace_with_your_hex_private_key
# RPC endpoint for the selected testnet
ETH_RPC_URL=https://your-testnet-rpc
# Chain ID for the selected testnet (e.g., 80002 for Polygon Amoy)
ETH_CHAIN_ID=80002
# Deployed DID registry contract address
ETH_REGISTRY_ADDRESS=0xYourDeployedContract
# Optional: DID to resolve for read-only test
ETH_RESOLVE_DID=did:byd50:12345555
