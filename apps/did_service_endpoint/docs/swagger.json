    "info": {
        "description": "This is a sample server for DID ServiceEndpoint.",
        "title": "DID Phase 2 Test API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
